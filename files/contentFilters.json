[{
		"summary": "6->陆",
		"rule": "(?<!\\d)6(?=[地离续])|(?<=[大着内])6 @=>陆"
	},
	{
		"summary": "cha->插",
		"rule": "cha(?=[槽头座队]) @=>插"
	},
	{
		"summary": "chou->春",
		"rule": "chou(?=[天风雨季雷])|chou(?=[如]) @=>春"
	},
	{
		"summary": "dang->荡",
		"rule": "(?<=[空晃]|dang)dang @=>荡"
	},
	{
		"summary": "dang->动",
		"rule": "(?<=[震振])dang @=>动"
	},
	{
		"summary": "dong->洞",
		"rule": "(?<=[空漏])dong @=>洞"
	},
	{
		"summary": "hua->花",
