	},
	{
		"summary": "hun->魂",
		"rule": "(?<=[精灵])hun|hun(?=魄) @=>魂"
	},
	{
		"summary": "huo->惑",
		"rule": "(?<=[迷困疑蛊所])huo @=>惑"
	},
	{
		"summary": "ji->激",
		"rule": "(?<=[感冲刺过])ji|ji(?=[动烈进昂战发怒增将化荡活情励]) @=>激"
	},
	{
		"summary": "jīng->精",
		"rule": "(jīng|jing|\\*)(?=[力神灵怪魄确魂气血锐]) @=>精"
	},
	{
		"summary": "jiao->交",
		"rule": "jiao(?=[流给待火]) @=>交"
	},
	{
		"summary": "jianyin->奸淫",
		"rule": "jianyin @=>奸淫"
	},
	{
		"summary": "luo->祼",
		"rule": "(?<=赤)luo|luo(?=[露睡体奔]) @=>祼"
	},
	{
		"summary": "mén->门",
		"rule": "mén @=>门"
	},
	{
		"summary": "nòng->弄",
		"rule": "(?<=[捉玩摆])n[oò]ng|n[oò]ng(?=[虚手臣明点权]) @=>弄"
	},
	{
		"summary": "nv->女",
		"rule": "nv(?=[人孩子士儿子官装帝王奴])|(?<=[一男儿淑美少])nv @=>女"
	},
	{
		"summary": "sao->骚",
		"rule": "sao(?=[动气乱扰气客话])|(?<=[发牢风])sao @=>骚"
	},
	{
		"summary": "sè->色",
		"rule": "sè @=>色"
	},
	{
		"summary": "shè->射",
		"rule": "(?<=[弹发暗影注喷反辐放])shè|shè(?=[箭日击程手]) @=>射"
	},
	{
		"summary": "tui->腿",
		"rule": "(?<=[大小])tui|tui(?=[部上软]) @=>腿"
	},
	{
		"summary": "xing->性",
		"rule": "(?<=[属血人能攻害征死])xing|xing(?=[情能格致质]) @=>性"
	},
	{
		"summary": "yin->淫",
		"rule": "(?<=白日宣|奇)yin|yin(?=[邪僧贼]) @=>淫"
	},
	{
		"summary": "yin->阴",
		"rule": "(?<=[太光遮])y[iī]n|y[iī]n(?=[沉阳历霾暗干面凉影谋毒险湿天魂文晦冷骘]) @=>阴"
	},
	{
		"summary": "zhèng->正",
		"rule": "(?<=[真])zhèng @=>正"
	},
	{
		"summary": "zhù->助",
		"rule": "(?<=[求帮])zhù @=>助"
	},
	{
		"summary": "**->八九",
		"rule": "(?<=[之九]|十有)\\*\\*|\\*\\*(?=[岁年月日周天个米丈里层成熟点]|不离十) @=>八九"
	},
	{
		"summary": "**->被操",
		"rule": "\\*\\*(?=[纵控]) @=>被操"
	},
	{
		"summary": "**->国军",
		"rule": "(?<=[帝俄法])\\*\\*|\\*\\*(?=[队务需人]) @=>国军"
	},
	{
		"summary": "巨*->巨大",
		"rule": "巨\\* @=>巨大"
	},
	{
		"summary": "**->赤裸",
		"rule": "\\*\\*(?=裸) @=>赤裸"
	},
	{
		"summary": "***->草泥马",
		"rule": "(?<=[头匹])\\*\\*\\* @=>草泥马"
	}
]
